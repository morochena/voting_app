<header class="masthead text-center text-white">
      <div class="masthead-content">
        <div class="container">
          <h1 class="masthead-heading mb-0">This Or That</h1>
          <h2 class="masthead-subheading mb-0">The best place to ask silly questions and vote.</h2>
          <%= link_to 'Sign Up', signup_path, class: "btn btn-primary btn-xl rounded-pill mt-5" %>
        </div>
      </div>
      <div class="bg-circle-1 bg-circle"></div>
      <div class="bg-circle-2 bg-circle"></div>
      <div class="bg-circle-3 bg-circle"></div>
      <div class="bg-circle-4 bg-circle"></div>
</header>

<section>
  <div class="container">
    <div class="row">

      <div class="col-lg-4">
        <div class="p-5">
          <%= image_tag ("https://img.cinemablend.com/filter:scale/cb/f/7/5/b/1/f/f75b1fe58a3cece5c4d532783b70c0b6cc6765fb60933ce45f063248ac7d06d7.jpg?mw=600"), class: "img-fluid rounded" %>
          <h3>Would you rather be friends with Godzilla, Captain Planet or King Kong?</h3>
          <%= bar_chart({"Godzilla" => 128, "Captain Planet" => 150, "King Kong" => 300},
              min: 0,)
          %>
        </div>
      </div>

      <div class="col-lg-4">
        <div class="p-5">
          <%= image_tag ("http://i.imgur.com/GPNpJnF.jpg"), class: "img-fluid rounded" %>
          <h3>Which Dark Souls character is the the best?</h3>
          <%= bar_chart({
            "Patches" => 0,
            "Solaire" => 300,
            "Blacksmith Andre" => 40,
            "Vendrick" => 290,
            "Anri" => 85,
            "Siegward" => 250,
            },
              min: 0,)
          %>
        </div>
      </div>

      <div class="col-lg-4">
        <div class="p-5">
          <%= image_tag ("data:image/jpeg;base64,"), class: "img-fluid rounded" %>
          <h3>What's your favorite Overwatch character?</h3>
          <%= bar_chart({"McCree" => 3000, "Pharah" => 1},
              min: 0,)
          %>
        </div>
      </div>

    </div>
  </div>
</section>
